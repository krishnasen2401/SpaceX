<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lingamworks.spacex">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@drawable/ic_spacex_original"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_spacex_original"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpaceX">
        <activity
            android:name=".Activity.Mainscreen"
            android:label="@string/title_activity_mainscreen"
            android:theme="@style/Theme.SpaceX.NoActionBar"></activity>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>